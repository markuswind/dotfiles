[core]
  excludesfile = /Users/markus/.gitignore_global
[user]
  name = Markus Wind
  email = markuswind@gmail.com
[push]
  default = matching
[alias]
  s = status
  head = !git tree -1
  tree = !GIT_NO_PAGER=1 git largetree -30
  largetree = "!. ~/.githelpers && pretty_git_log"
[color]
  branch = auto
  diff = auto
  status = auto
[color "branch"]
  current = yellow reverse
  local = yellow
  remote = green
[color "diff"]
  meta = yellow bold
  frag = magenta bold
  old = red bold
  new = green bold
[color "status"]
  added = yellow bold
  deleted = red
  changed = green
  untracked = cyan
